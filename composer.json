{
    "name": "dmitryrechkin/php-graphql-fieldresolver",
	"type": "php-graphql-fieldresolver",
    "description": "Field Resolver for GraphQL PHP",
    "keywords": [
    ],
    "license": "AGPL-3.0-only",
    "authors": [
        {
            "name": "Dmitry Rechkin",
            "email": "rechkin@gmail.com",
            "homepage": "https://github.com/dmitryrechkin/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3 || ^8.0",
        "webonyx/graphql-php": "^14.11",
        "php-di/php-di": "^6.4"
    },
    "require-dev": {
        "dmitryrechkin/php-standard": "^1.0",
        "dmitryrechkin/php-phpunit-bootstrap": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DmitryRechkin\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DmitryRechkin\\Tests\\" : "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
